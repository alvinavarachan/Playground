#include<stdio.h>
int main()
{
 int n,k=0;;
 scanf("%d",&n);
  while(n!=0)
  {
    int i=n%10;
       int c=0;
       for(int j=1;j<=i;j++)
       {
           if(i%j==0)
           {
               c++;
           }
       }
        
      if(c==2)
       {
          k=k+i;
       }
    n=n/10;
   }
printf("%d",k);

   return 0;
}
